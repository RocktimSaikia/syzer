	"description": "Updates package.json with latest dependency versions",
	"license": "MIT",
	"bin": {
		"syzer": "lib/cli.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rocktimsaikia/syzer.git"
	},
	"author": {
		"name": "Rocktim Saikia",
		"email": "rocktimthedev@gmail.com",
		"url": "https://rocktimsaikia.me"
	},
	"engines": {
		"node": ">=10"
	},
	"files": [
		"lib"
	],
	"keywords": [
		"cli",
		"update",
		"dependencies",
		"npm",
		"syzer",
		"update",
		"packages"
	],
	"scripts": {
		"start": "babel-node src/cli.js",
